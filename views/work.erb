<div class="container">
	<h1 class="page-header"><%= @title %></h1>
	<div class="all-projects">
		<% @jobs.each do |job| %>
			<div class="project">
				<h2 class="sub-header" id="<%= job.company_name %>"><a href="work/<%= job.id %>"><%= job.company_name %></a></h2>
				<p class="project-desc"><%= job.job_title %></p>
			</div>
		<% end %>
	</div>
</div>